    "rollup": "^4.22.0",
    "rollup-plugin-dts": "^6.1.1",
    "rimraf": "^5.0.5",
    "semantic-release": "^24.0.0",
    "tslib": "^2.6.2",
    "typescript": "^5.6.2",
    "vite": "^5.2.0",
    "vitest": "^1.6.0",
    "tailwind-merge": "^2.5.2",
    "storybook": "^8.4.0"
